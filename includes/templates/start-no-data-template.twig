<div id="api-start-message">
    <div class="col-xl-10 col-12 mx-auto">
        <div class="px-4 pt-3 mt-3 pb-5 mb-5 text-center d-flex flex-column  position-relative">
            <div><a target="_blank" href="https://www.hummelt-werbeagentur.de/">
                    <i class="hupa-color icon-hupa-white hupa-scale-img fa-5x d-inline-block text-center py-2"></i>
                </a>
                <h1 class="" style="font-weight: 400;letter-spacing: -.5px;"><b class="text-google">G</b>oogle
                    {{ data.l.reviews }}
                </h1>
                <div class="col-lg-8 mx-auto">
                    <p class="lead  mb-4">
                        {{ data.l.no_data_info }}
                    </p>
                </div>
            </div>
        </div>
    </div>
    <div class="alert-no-key text-muted px-lg-5 {% if not data.error %}d-none{% endif %}">
        <i class="fa fa-exclamation-triangle mx-1"></i> {{ data.l.no_api_key_found }}!
        <hr class="mt-1 mb-2">
        {% autoescape false %}
            <p class="text-start ms-2 position-relative">
                <i class="position-absolute fa fa-info-circle mt-1 start-0"></i>
                <span class="d-block ms-3"> {{ data.l.no_key_info_one }}</span>
            </p>
            <p class="text-start ms-2 position-relative">
                <i class="position-absolute fa fa-info-circle mt-1 start-0"></i>
                <span class="d-block ms-3"> {{ data.l.no_key_info_second }}</span>
            </p>
        {% endautoescape %}
        <hr class="mt-2 mb-2">
    </div>
</div>